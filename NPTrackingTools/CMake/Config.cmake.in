# - Config file for the @PROJECT_NAME@ package

# It defines the following variables
#  @PROJECT_NAME@_INCLUDE_DIRS - include directories for @PROJECT_NAME@
#  @PROJECT_NAME@_LIBRARIES    - libraries to link against
#  @PROJECT_NAME@_EXECUTABLE   - the @PROJECT_NAME@ executable

# Create Variables for use in other projects
set(@PROJECT_NAME@_INCLUDE_DIRS "@CONF_INCLUDE_DIRS@")

set(@PROJECT_NAME@_INCLUDE_FILES )
foreach(INCLUDE_DIR ${@PROJECT_NAME@_INCLUDE_DIRS})
    # Include files
    file(GLOB CONF_INCLUDE_FILES_AUX ${INCLUDE_DIR}/*.h)
    set(@PROJECT_NAME@_INCLUDE_FILES ${@PROJECT_NAME@_INCLUDE_FILES} ${CONF_INCLUDE_FILES_AUX})
endforeach()

set(@PROJECT_NAME@_SOURCE_DIRS @CONF_SOURCE_DIRS@)
set(@PROJECT_NAME@_LIBRARY @CONF_LIBRARY@)
set(@PROJECT_NAME@_LIBRARY_DIRS @CONF_LIBRARY_DIRS@)
set(@PROJECT_NAME@_BINARY_DIR @CONF_BINARY_DIR@)
set(@PROJECT_NAME@_INSTALL_DIR @CONF_INSTALL_DIR@)
set(@PROJECT_NAME@_SHARED @CONF_SHARED@)
set(@PROJECT_NAME@_DEFINITIONS @CONF_DEFINITIONS@)
set(@PROJECT_NAME@_EXECUTABLES_DIRS "@CONF_EXECUTABLES_DIRS@")

set(@PROJECT_NAME@_EXECUTABLES )
foreach(EXECUTABLE_DIR ${@PROJECT_NAME@_EXECUTABLES_DIRS})
    # Executable files
    file(GLOB CONF_EXECUTABLE_FILES_AUX ${EXECUTABLE_DIR}/*)
    set(@PROJECT_NAME@_EXECUTABLES ${@PROJECT_NAME@_EXECUTABLES} ${CONF_EXECUTABLE_FILES_AUX})
endforeach()

# Our library dependencies (contains definitions for IMPORTED targets)
include("${@PROJECT_NAME@_BINARY_DIR}/@PROJECT_NAME@Targets.cmake")